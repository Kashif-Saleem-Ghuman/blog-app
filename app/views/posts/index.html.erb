<article>
  <section class="user">
  <img class="image" src="<%=@users.photo %>" alt="User Avatar" />
    <div class="user-info">
      <h2 class="user-name"><%= @users.name %></h2>
      <p class="post-count">Number of posts: <%= @users.posts_counter%> </p>
    </div>
  </section>
    
</article>