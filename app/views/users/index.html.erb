<article class="user-container">
  <% @users.each do |user| %>
      
        <section class="user">
          <%= user.photo %>
            <div class="user-info">
              <h2><%= user.name %></h2>
              Number of posts: <%= user.posts_counter %>
              
            </div>
        </section>
    
  <% end %>
</article>


